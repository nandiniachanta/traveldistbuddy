{
  "compilerOptions": {
    "target": "esnext",
    "lib": [ "dom", "dom.iterable", "esnext" ],
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "include": [ "src" ]
}
